{
  "name": "tanstack-practice",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "component-test": "playwright test -c playwright-ct.config.ts",
    "component-test:dev": "rm -rf ./playwright/.cache && playwright test -c playwright-ct.config.ts --ui",
    "update-types": "supabase gen types typescript --project-id \"$PROJECT_REF\" > src/schema.ts",
    "safetest": "OPT_URL='http://localhost:5173/' vitest --config vitest.safetest.config --poolOptions.threads.singleThread",
    "safetest:ci": "rm -f artifacts.json && OPT_URL=${DEPLOYED_URL} OPT_CI=1 OPT_DOCKER=1 OPT_ARTIFACTS=artifacts.json npm run safetest -- --run --bail=5",
    "safetest:regenerate-screenshots": "OPT_DOCKER=1 npm run safetest -- --run --update"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.7",
    "@tanstack/react-query": "^5.21.4",
    "@tanstack/react-router": "^1.16.5",
    "axios": "^1.6.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "supabase": "^1.142.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@playwright/experimental-ct-react": "^1.41.2",
    "@tanstack/react-query-devtools": "^5.21.5",
    "@tanstack/router-devtools": "^1.16.5",
    "@tanstack/router-vite-plugin": "^1.16.5",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "msw": "^2.2.1",
    "playwright-msw": "^3.0.1",
    "postcss": "^8.4.35",
    "safetest": "^0.1.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.1"
  }
}
